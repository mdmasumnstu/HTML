    <!--Form Tag -->
        <h2>Registration From</h2> <br>
        <form>
           <div>
                <level for="fullname">Full Name</level>
                <input type="text" name="fullname" id="fullname" required>
           </div><br>

           <div>
                <level for="password">Email</level>
                <input type="email" name="email" id="email" required>
            </div><br>

            <div>
                <level for="password">Password</level>
                <input type="password" name="password" id="password">
           </div><br>

           <div>
                <level for="dob">Date of Birth</level>
                <input type="date" name="dob" id="dob">
            </div><br>

            <div>
                <level for="photo">Choose of Your Image</level>
                <input type="file" name="photo" id="photo">
           </div><br>

           <div>
                <level for="gender">Gender</level>
                <input type="radio" name="gender" id="gender" value="male">Male
                <input type="radio" name="gender" id="gender" value="female">Female
            </div><br>

            <div>
                <level for="religion">Religion</level>
                <input checked type="checkbox" name="religion" id="religion" value="islam">Ialam

                <input type="checkbox" name="religion" id="religin" value="Hindu">Hindu

                <level for="religion">Religion</level>
                <input type="checkbox" name="religion" id="religion" value="christan">Christan

                <input type="checkbox" name="religion" id="religin" value="Other">Other

            </div><br>

            <div>
                <level>Department</level>
                <Select for="Department">
                    <option value="cse">CSE</option>
                    <option value="ict">ICT</option>
                    <option value="math" selected>Mathematics</option>
                    <option value="chemistry">Chemistry</option>
                </Select>
            </div> <br>

            <div>
